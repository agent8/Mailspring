  "navigation:go-to-all-inbox": "i",
  "multiselect-list:select-all": "shift+x",

  "core:pop-sheet": "u",
  "core:previous-item": "k",
  "core:select-up": "shift+k",
  "core:next-item": "j",
  "core:select-down": "shift+j",
  "core:focus-item": "o",


  "core:focus-search": "/",
  "core:change-folders": ".",
